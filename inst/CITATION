year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype = "Manual",
         title = "{touRism}: Tools for Analysis of Tourism Demand",
         author = c(person("Marius", "Larsson", "Runningen")),
         year = year,
         note = note,
         url = "https://github.com/mariuslarsson/touRism")
